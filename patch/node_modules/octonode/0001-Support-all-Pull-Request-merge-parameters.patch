From 559202be4502879f8e0e57d68b779931522131bb Mon Sep 17 00:00:00 2001
From: Michael Vines <mvines@gmail.com>
Date: Sat, 26 May 2018 21:36:19 -0700
Subject: [PATCH] Support all Pull Request merge parameters

diff --git a/lib/octonode/pr.js b/lib/octonode/pr.js
index afe7d77..ce39822 100644
--- a/lib/octonode/pr.js
+++ b/lib/octonode/pr.js
@@ -56,11 +56,15 @@
       });
     };
 
-    Pr.prototype.merge = function(msg, cb) {
+    Pr.prototype.merge = function(msgOrCommitObject, cb) {
       var commit;
-      commit = {
-        commit_message: msg
-      };
+      if (typeof msgOrCommitObject === 'object') {
+        commit = msgOrCommitObject;
+      } else {
+        commit = {
+          commit_message: msgOrCommitObject,
+        };
+      }
       return this.client.put("/repos/" + this.repo + "/pulls/" + this.number + "/merge", commit, function(err, s, b, h) {
         if (err) {
           return cb(err);
-- 
